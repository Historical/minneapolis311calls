extends layout

block content
    h1 Address Search Results
    div 
        p You Search for: #{addressID}
    div
        p Here is what we found!
    div.col-lg-12
    p Property Manager: 
        a#landlordName #{results[0].ContactName}
    p Manager since: #{results[0].IssueDate}

    div
    table.table.table-striped
      thead
        tr
          th #
          th Type
          th Date of Incident
      tbody
        each rrow, i in results
          tr
            td #{i+1}
            td #{results[i].TypeName} 
            td #{results[i].OpenedDateTime}

block script
    script(type='text/javascript').
        $(document).ready(function() {
                $('#landlordName').click(function(){
                   //alert("clicked");
                   var name = $('#landlordName').text();
                   //alert(name);
                   window.location.href = '/landlordSearch/'+name;
                });
            });